{
    "name": "fourlabs/robots-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to control X-Robots-Tag HTTP header via annotations",
    "keywords": ["google", "robots", "header", "x-robots-tag", "meta", "crawling"],
    "homepage": "http://fourlabs.co.uk",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Four Labs and other contributors",
            "homepage": "https://github.com/foaly-nr1/FourLabsEsoftBundle/graphs/contributors"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.4 || ~3.0",
        "doctrine/annotations": "~1.0"
    },
    "autoload": {
        "psr-4": { "FourLabs\\RobotsBundle\\": "" }
    }
}
